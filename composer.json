    "keywords": ["vanilla","queue","worker","distributed"],
    "homepage": "https://github.com/vanilla/queue-worker",
    "type": "library",
    "license": "proprietary",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Tim Gunter",
            "email": "tim@vanillaforums.com",
            "homepage": "https://vanillaforums.com"
        }
    ],
    "repositories": [
