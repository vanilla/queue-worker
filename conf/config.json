{
    "daemon": {
        "loglevel": "info",
        "runasgroup": "root",
        "runasuser": "root",
        "concurrent": false,
        "daemonize": true,
        "mode": "fleet",
        "fleet": 15
    },
    "process": {
        "sleep": 500,
        "max_retries": 3,
        "max_requests": 100
    },
    "queue": {
        "oversight": {
            "strategy": "\\Vanilla\\ProductQueue\\Allocation\\HybridStrategy",
            "frequency": 30,
            "adjust": 5
        },
        "message": {
            "parser": "\\Vanilla\\ProductQueue\\Message\\Parser\\JSONMessageParser"
        },
        "queues": [
            {
                "name": "priority",
                "priority": 0,
                "min": 2
            },
            {
                "name": "normal",
                "priority": 1,
                "min": 2
            }
        ]
    },
    "addons": {
        "scan": [
            "addons/"
        ]
    },
    "cache": {
        "prefix": "productqueue"
    },
    "log": [
        {
            "destination": "echo",
            "level": "debug",
            "key": "echo"
        },
        {
            "destination": "filesystem",
            "level": "debug",
            "file": "log/daemon.log",
            "key": "persist"
        }
    ],
    "lib": [
        {
            "name": "data-jobs",
            "path": "/var/www/api/applications/vfdata",
            "libraries": ["jobs"]
        }
    ]
}
